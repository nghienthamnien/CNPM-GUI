{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "airbnb", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "no-console": "off",
        "react/function-component-definition": [0],
        "react/prop-types": [0]
    },
    "ignorePatterns": ["assets", "*.css", "landing-page", "blog"]
}
